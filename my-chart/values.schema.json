{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "autoscaling": {
      "additionalProperties": false,
      "properties": {
        "enabled": {
          "default": false,
          "required": [],
          "title": "enabled",
          "type": "boolean"
        },
        "maxReplicas": {
          "default": 100,
          "required": [],
          "title": "maxReplicas",
          "type": "integer",
          "minimum": 10,
          "maximum": 100
        },
        "minReplicas": {
          "default": 1,
          "required": [],
          "title": "minReplicas",
          "type": "integer",
          "minimum": 1,
          "maximum": 5
        }
      },
      "required": [],
      "title": "autoscaling",
      "type": "object"
    },
    "image": {
      "additionalProperties": false,
      "properties": {
        "repository": {
          "default": "nginx",
          "required": [],
          "title": "repository",
          "type": "string"
        },
        "tag": {
          "default": "",
          "description": "Overrides the image tag whose default is the chart appVersion.",
          "required": [],
          "title": "tag",
          "type": "string"
        }
      },
      "required": [
        "repository"
      ],
      "title": "image",
      "type": "object"
    },
    "ingress": {
      "additionalProperties": false,
      "properties": {
        "enabled": {
          "default": false,
          "required": [],
          "title": "enabled",
          "type": "boolean"
        }
      },
      "required": [],
      "title": "ingress",
      "type": "object"
    },
    "replicaCount": {
      "default": 1,
      "minimum": 1,
      "maximum": 15,
      "required": [],
      "title": "replicaCount",
      "type": "integer"
    },
    "service": {
      "additionalProperties": false,
      "properties": {
        "port": {
          "default": 80,
          "required": [],
          "title": "port",
          "type": "integer"
        },
        "type": {
          "title": "type",
          "type": "string",
          "enum": [
            "ClusterIP",
            "NodePort",
            "LoadBalancer"
          ]
        }
      },
      "required": [
        "type",
        "port"
      ],
      "title": "service",
      "type": "object"
    },
    "serviceAccount": {
      "additionalProperties": false,
      "properties": {
        "create": {
          "default": true,
          "description": "Specifies whether a service account should be created",
          "required": [],
          "title": "create",
          "type": "boolean"
        }
      },
      "required": [
        "replicaCount"
      ],
      "title": "serviceAccount",
      "type": "object"
    }
  },
  "type": "object"
}